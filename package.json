    "test-check-broken-links": "node ./test/check-broken-links",
    "test-eslint": "eslint --ext .js --ext .jsx .",
    "watch": "npm-run-all --parallel watch-js watch-js-server",
    "watch-js": "npm run build-js -- --watch",
    "watch-js-server": "npm run build-js-server -- --watch"
  },
